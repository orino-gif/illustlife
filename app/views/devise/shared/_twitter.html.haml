- if devise_mapping.omniauthable?
  - resource_class.omniauth_providers.each do |provider|
    = link_to image_tag('/img/twitter.png',class:"size_small-square"), omniauth_authorize_path(resource_name, provider), method: :post
    // = link_to t("　ツイッターアカウントで登録", provider: OmniAuth::Utils.camelize(provider)), omniauth_authorize_path(resource_name, provider), method: :post